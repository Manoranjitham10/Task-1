def reduceTicketPrice(ticket, k):
    stack = []
    for digit in ticket:
        while k > 0 and stack and stack[-1] > digit:
            stack.pop()
            k -= 1
        stack.append(digit)
    
    # If k is still greater than 0, remove the remaining digits from the end
    while k > 0:
        stack.pop()
        k -= 1
    
    # Convert the stack back to a string and remove leading zeros
    result = ''.join(stack).lstrip('0')
    
    # If the result is empty, return '0'
    return result if result else '0'

# Get user input
ticket = input("Enter the ticket price: ")
k = int(input("Enter the number of digits to remove: "))

# Calculate and print the reduced ticket price
print("Reduced ticket price:", reduceTicketPrice(ticket, k))
